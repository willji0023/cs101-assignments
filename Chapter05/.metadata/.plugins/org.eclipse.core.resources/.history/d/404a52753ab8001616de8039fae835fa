#include<iostream>
#include<cmath>
#include<vector>

using namespace std;

double roottest (double a, b, c)
{
	double root1 = sqrt((pow(b, 2)-(4*a*c)), 2);
	return root1;
}

int main()
{
	double a, b, c = 0;
	cout << "Enter your a value followed by b value followed by c.\n";
	cin >> a >> b >> c;
	double root1 = roottest(a, b, c);
	cout << root1
}


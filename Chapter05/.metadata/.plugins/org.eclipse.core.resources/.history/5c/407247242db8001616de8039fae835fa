#include<iostream>
#include<algorithm>
#include<string>
#include<vector>
#include<cmath>
using namespace std;
inline void keep_window_open() { char ch; cin>>ch; }

int main()
try {
 <<your code here>>
 keep_window_open();
 return 0;
}
catch (exception& e) {
 cerr << "error: " << e.what() << '\n';
 keep_window_open();
 return 1;
}
catch (…) {
 cerr << "Oops: unknown exception!\n";
 keep_window_open();
 return 2;
}
